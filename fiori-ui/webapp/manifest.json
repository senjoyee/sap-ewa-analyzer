{
  "_version": "2.0.0",
  "sap.app": {
    "id": "ewa.analyzer",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "0.0.1"
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ewa.analyzer.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.136.0",
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ewa.analyzer.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "path": "ewa.analyzer.view",
        "controlId": "appLayout",
        "controlAggregation": "beginColumnPages"
      },
      "routes": [
        {
          "name": "Main",
          "pattern": "",
          "target": ["Main"]
        },
        {
          "name": "Preview",
          "pattern": "preview/{baseName}",
          "target": ["Preview"]
        }
      ],
      "targets": {
        "Main": {
          "name": "FileList",
          "id": "fileList",
          "level": 1,
          "controlAggregation": "beginColumnPages"
        },
        "Preview": {
          "name": "Analysis",
          "id": "analysis",
          "level": 2,
          "controlAggregation": "endColumnPages"
        }
      }
    }
  }
}
